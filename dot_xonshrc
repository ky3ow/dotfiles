import xsh.helpers as h

_module_ensures = [
    h.Xontrib("coreutils"),
    h.Xontrib("argcomplete"),
    h.Xontrib("abbreviations"),
]

for _module in _module_ensures:
    _module.ensure()


_CONFIG_DIR = p"~/.config/xonsh"

source @(_CONFIG_DIR / "profile.xsh")
source @(_CONFIG_DIR / "main.xsh")
